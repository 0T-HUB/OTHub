{
  "name": "sveltekit-starter",
  "version": "0.0.1",
  "scripts": {
    "dev": "./node_modules/.bin/svelte-kit dev",
    "build": "./node_modules/.bin/svelte-kit build",
    "package": "./node_modules/.bin/svelte-kit package",
    "preview": "./node_modules/.bin/svelte-kit preview",
    "check": "./node_modules/.bin/svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "./node_modules/.bin/svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "./node_modules/.bin/prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "./node_modules/.bin/prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-vercel": "^1.0.0-next.50",
    "@sveltejs/kit": "next",
    "@tailwindcss/jit": "^0.1.18",
    "@types/cookie": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "autoprefixer": "^10.4.5",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.47.0",
    "svelte-bootstrap-icons": "^1.8.1",
    "svelte-check": "^2.7.0",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3"
  },
  "type": "module",
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.22.1",
    "@fontsource/fira-mono": "^4.5.8",
    "@lukeed/uuid": "^2.0.0",
    "chart.js": "^3.8.0",
    "cookie": "^0.5.0",
    "svelte-chartjs": "^1.1.4",
    "svelte-particles": "^2.0.6",
    "svelte-web3": "^3.5.0"
  }
}
